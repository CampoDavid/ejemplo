# ✅ Archivo de ejemplo de entorno (.env.example)

# Nombre de la base de datos MySQL
DB_NAME=tutorias

# Usuario de base de datos
DB_USER=root

# Contraseña del usuario (dejar en blanco si no tiene)
DB_PASSWORD=

# Host de la base de datos (localhost para XAMPP o Workbench local)
DB_HOST=localhost

# Puerto de la API
PORT=3000

# Clave secreta para JWT
JWT_SECRET=secreto
